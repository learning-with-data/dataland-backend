{
  "name": "dataland-backend",
  "description": "Back-end server for the Data Land project",
  "version": "0.1.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "license": "BSD-3-Clause",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Sayamindu Dasgupta",
    "email": "sayamindu@unc.edu"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "coverage": "nyc npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "nodemon src/",
    "start": "node src/",
    "mocha": "NODE_ENV=test mocha test/ --recursive --exit",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "feathers-authentication-hooks": "^1.0.2",
    "feathers-hooks-common": "^5.0.6",
    "feathers-sequelize": "^6.2.0",
    "generate-password": "^1.7.0",
    "helmet": "^5.0.2",
    "mysql2": "^2.3.0",
    "sequelize": "^6.7.0",
    "serve-favicon": "^2.5.0",
    "unique-names-generator": "^4.7.1",
    "winston": "^3.3.3",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "eslint": "^8.0.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0"
  }
}
