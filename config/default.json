{
  "host": "DATALAND_BACKEND_HOST",
  "port": "DATALAND_BACKEND_PORT",
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "DATALAND_BACKEND_SECRET",
    "authStrategies": [
      "jwt",
      "local"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "usernameField": "username",
      "passwordField": "password"
    }
  },
  "mysql": "DATALAND_BACKEND_DB_URI",
  "invitation_code": "DATALAND_BACKEND_INVITATION_CODE"
}
